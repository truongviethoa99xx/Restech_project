</head>
<body>
    <h2>Welcome to my world</h2>
    <p>This is VicNalz's world</p>
    <p id="demo"></p>
    <script>
        let time = new Date() .getHours();
        let greeting;
        if (time < 10) {
            greeting = "Good Morning";
        }   else if ( time < 20) {
            greeting = "Have a great day";
        }   else {
            greeing = "Good Evening"
        }
        document.getElementById("demo").innerHTML = greeting;
    </script>
</body>
